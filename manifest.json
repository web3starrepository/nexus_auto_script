{
    "manifest_version": 3,
    "name": "Nexus Auto Connect",
    "version": "1.0.0",
    "description": "自动连接Nexus服务",
    "host_permissions": [
        "*://app.nexus.xyz/*"
    ],
    "permissions": [
        "alarms",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    }
}